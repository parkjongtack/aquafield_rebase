<%@ page contentType="text/html; charset=utf-8" pageEncoding="utf-8"%><%@ include file="../../common/taglibs.jsp" %><script type="text/javascript">	// 목록버튼  	function goListPage(){		document.serach_form.submit();	}	// 저장버튼	function goUpdate(){		var frm = document.update;		//이메일 정규식		var reg_email=/^[-A-Za-z0-9_]+[-A-Za-z0-9_.]*[@]{1}[-A-Za-z0-9_]+[-A-Za-z0-9_.]*[.]{1}[A-Za-z]{2,5}$/;		//ID Email 입력여부 검사		if(frm.mem_id.value==""){			alert( "E-mail 을 입력해주시기 바랍니다.")			frm.mem_id.focus()			return false;		}		// email 검사		if(reg_email.test(frm.mem_id.value) === false) { 			alert( '잘못된 형식의 E-mail 입니다.');			frm.mem_id.focus()			return false; 		}		frm.submit();	}		//고개문의 탭	function goMemberCS(){		var frm = document.update;		frm.action = "/admin/member/member_cs_list.af";		frm.submit();	}		//예약내역 탭	function goMemberRES(){		var frm = document.update;		frm.action = "/admin/member/member_res.af";		frm.submit();	}	</script><section id="contents">	<div class="contents_bx_type1">		<div class="contents_bx_inner">			<div id="path" class="fixed">				<div class="pathInner">					<h2>회원정보</h2>					<div class="btns_area">						<button class="btn_pack btn_mo d_gray" type="button" onclick="goUpdate();"><img src="/common/admin/images/common/ico_save.png">저장</button>						<a href="#" onclick="goListPage();" class="btn_pack btn_mo gray"><img src="/common/admin/images/common/ico_list.png">목록</a>					</div>				</div>			</div>			<div class="tabMenu">	 			<ul class="clearfix">	 				<li class="on"><a href="javascript:void(0);">회원정보</a></li>	 				<li><a href="#" onclick="goMemberRES(); return false;">예약내역</a></li>	 				<li><a href="#" onclick="goMemberCS(); return false;">고객문의</a></li>	 			</ul>	 		</div>	 		<form name="update" action="/admin/member/member_update.af" method="post">	 			<input type="hidden" name="mem_uid" value="${vo.MEM_UID }">		 		<div class="tb_type2 mb20 tabContent">					<table>						<tbody>							<tr>								<th>회원번호</th>								<td>${vo.MEM_UID}</td>								<th>가입일</th>								<td>${vo.INS_DATE}</td>							</tr>							<tr>								<th>회원명</th>								<td>${vo.MEM_NM}</td>								<th>이메일/SMS/DM<br/>정보수신 동의</th>								<td>${vo.RECEIVED_INFO_AT}</td>							</tr>							<tr>								<th>휴대폰번호</th>								<td>${vo.MOBILE_NUM}</td>								<th>자택 전화번호</th>								<td>${vo.HOME_PHONE_NUM}</td>							</tr>							<tr>								<th>아이디(이메일)</th>								<td>									<input type="text" name="mem_id" class="ipt2 w140" value="${vo.MEM_ID}" />								</td>								<th>직장명</th>								<td>${vo.COMPANY_NM}</td>							</tr>							<tr>								<th>생년월일</th>								<td>${vo.MEM_BIRTH} 									<c:if test="${vo.BIRTH_TYPE eq 'Y'}">양력</c:if>									<c:if test="${vo.BIRTH_TYPE ne 'Y'}">음력</c:if>								</td>								<th>직장주소</th>								<td>${vo.COMPANY_ADDR1} ${vo.COMPANY_ADDR2}</td>							</tr>							<tr>								<th>성별</th>								<td>									<c:if test="${vo.MEM_GENDER eq 'M'}">남</c:if>									<c:if test="${vo.MEM_GENDER eq 'W'}">여</c:if>								</td>								<th>직장 전화번호</th>								<td>${vo.COMPANY_PHONE_NUM}</td>							</tr>							<tr>								<th>주소</th>								<td>${vo.HOME_ADDR1} ${vo.HOME_ADDR2}</td>								<th>직업</th>								<td>${vo.JOB_NM}</td>							</tr>							<tr>								<th>기념일</th>								<td>${vo.MEMORY_DAY}</td>								<th>&nbsp;</th>								<td>&nbsp;</td>							</tr>						</tbody>					</table>				</div>			</form>		</div>	</div></section><form name="serach_form" action="/admin/member/index.af" method="post" >	<input type="hidden" name="order_res" value="${resultParam.order_res }">	<input type="hidden" name="page" value="${resultParam.page }">	<input type="hidden" name="srch_text" value="${resultParam.srch_text }">	<input type="hidden" name="srch_sex" value="${resultParam.srch_sex }">	<input type="hidden" name="srch_reg_s" value="${resultParam.srch_reg_s }">	<input type="hidden" name="srch_reg_e" value="${resultParam.srch_reg_e }"></form>