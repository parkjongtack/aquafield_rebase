<%@ page contentType="text/html; charset=utf-8" pageEncoding="utf-8"%><%@ include file="../../common/taglibs.jsp" %><section id="notice_view">	<header>        <div class="titleArea">        	<div class="tag"><img class="ico" src="/common/front/images/ico/ico_news_notice.png"> ${result.KIND_NAME}</div>        	<h1 class="title">${result.TITLE}</h1>        	<ul class="info">        		<li><span>작성일: </span>${result.INST_DATE_TXT}</li>        	<c:if test="${result.KIND eq '2'}">        		<li><span>기간: </span>${result.START_DATE} ~ ${result.END_DATE}</li>        		<li><span>장소: </span>${result.LOCATION}</li>        	</c:if>        	</ul>        	<ul class="sns"><!--         		<li><a href="javascript:void(0);" class="link"></a></li> -->        		<li><a href="javascript:void(0);" class="facebook" onclick="sendSns('facebook','${result.TITLE}')"></a></li>        		<li><a href="javascript:void(0);" class="twitter" onclick="sendSns('twitter','${result.TITLE}')"></a></li>        		<li><a href="javascript:void(0);" class="kakao" onclick="sendSns('kakao','${result.TITLE}')"></a></li>        		<li><a href="javascript:void(0);" class="url" onclick="urlCopy()"></a></li>        	</ul>        </div>    </header>	<div class="iscContent">        <div class="inner">        	<article>        		<div class="newsContent">        			${result.CONTENTS}        		</div>        	</article>        </div>    </div>     <footer><c:if test="${AUTHWRITER eq 'W' }">           	<div class="btns center">        	<button class="btn_pack btn_mo white" onclick="$('.btn_close').trigger('click');location.href='#/event/write.af?page=${result.PAGE_TYPE_TXT}&num=${resultParam.num}'">수정</button>        	<button class="btn_pack btn_mo white" onclick="doDelete()">삭제</button>        </div></c:if>             </footer>   </section><script type="text/javascript">function doDelete(){	if(!confirm("삭제하시겠습니까? 복구될 수 없습니다.")) return;	$.ajax({		type:"POST",		url:"event/noticeEventDeleteAction.af",		dataType:"json",		data:{num:'${resultParam.num}'},		success : function(data) {			if(data.result){				alert(data.msg);				location.href="#/event/eventIndex.af?"+new Date().getTime();			}			else{				alert(data.msg);			}		},		error : function(xhr, status, error) {			alert("잠시 후에 다시하세요.");		}	});}</script>