<%@ page contentType="text/html; charset=utf-8" pageEncoding="utf-8"%><%@ include file="../../common/taglibs.jsp" %>
<section id="event_view">	<header>    	<c:if test="${not empty result.FILE_CONTENT_FULL}">    	<div class="imgArea">            <img src="${result.FILE_CONTENT_FULL}">           </div>        </c:if>        <div class="titleArea">            <div class="tag"><img class="ico" src="/common/front/images/ico/ico_news_event.png"> ${result.KIND_NAME}</div>            <h1 class="title">${result.TITLE}</h1>            <c:if test="${result.KIND eq '2'}">            <ul class="info">                <li><span>장소: </span>${result.LOCATION}</li>                <li><span>기간: </span>${result.START_DATE} ~ ${result.END_DATE}</li>            </ul>            </c:if>        	<ul class="sns"><!--         		<li><a href="javascript:void(0);" class="link"></a></li> -->        		<li><a href="javascript:void(0);" class="facebook" onclick="sendSns('facebook','${result.TITLE}')"></a></li>        		<li><a href="javascript:void(0);" class="twitter" onclick="sendSns('twitter','${result.TITLE}')"></a></li>        		<li><a href="javascript:void(0);" class="kakao" onclick="sendSns('kakao','${result.TITLE}')"></a></li>        		<li><a href="javascript:void(0);" class="url" onclick="urlCopy()"></a></li>        	</ul>        </div>    </header>	<div class="iscContent">        <div class="inner">        	<article>				<ul class="page-slider">                    <c:set var="pageNum" value="0" />                    <c:if test="${result.PAGE1_YN eq 'Y'}">                    <c:set var="pageNum" value="${pageNum + 1}" />                    <li class="page p${pageNum}">                        <c:if test="${result.PAGE1_LEFT_KIND eq 'text' && not empty result.PAGE1_LEFT_CONTENT}">                        <section class="txtArea">                            ${result.PAGE1_LEFT_CONTENT}                        </section>                        </c:if>                        <c:if test="${result.PAGE1_LEFT_KIND eq 'image' && not empty result.PAGE1_LEFT_IMG_FULL}">                        <section class="imgArea">                            <img src="${result.PAGE1_LEFT_IMG_FULL}">                        </section>                        </c:if>                        <c:if test="${result.PAGE1_RIGHT_KIND eq 'text' && not empty result.PAGE1_RIGHT_CONTENT}">                        <section class="txtArea">                            ${result.PAGE1_RIGHT_CONTENT}                        </section>                        </c:if>                        <c:if test="${result.PAGE1_RIGHT_KIND eq 'image' && not empty result.PAGE1_RIGHT_IMG_FULL}">                        <section class="imgArea">                            <img src="${result.PAGE1_RIGHT_IMG_FULL}">                        </section>                        </c:if>                    </li>                    </c:if>                    <c:if test="${result.PAGE2_YN eq 'Y'}">                    <c:set var="pageNum" value="${pageNum + 1}" />                    <li class="page p${pageNum}">                        <c:if test="${result.PAGE2_LEFT_KIND eq 'text' && not empty result.PAGE2_LEFT_CONTENT}">                        <section class="txtArea">                            ${result.PAGE2_LEFT_CONTENT}                        </section>                        </c:if>                        <c:if test="${result.PAGE2_LEFT_KIND eq 'image' && not empty result.PAGE2_LEFT_IMG_FULL}">                        <section class="imgArea">                            <img src="${result.PAGE2_LEFT_IMG_FULL}">                        </section>                        </c:if>                        <c:if test="${result.PAGE2_RIGHT_KIND eq 'text' && not empty result.PAGE2_RIGHT_CONTENT}">                        <section class="txtArea">                            ${result.PAGE2_RIGHT_CONTENT}                        </section>                        </c:if>                        <c:if test="${result.PAGE2_RIGHT_KIND eq 'image' && not empty result.PAGE2_RIGHT_IMG_FULL}">                        <section class="imgArea">                            <img src="${result.PAGE2_RIGHT_IMG_FULL}">                        </section>                        </c:if>                    </li>                    </c:if>                    <c:if test="${result.PAGE3_YN eq 'Y'}">                    <c:set var="pageNum" value="${pageNum + 1}" />                    <li class="page p${pageNum}">                        <c:if test="${result.PAGE3_LEFT_KIND eq 'text' && not empty result.PAGE3_LEFT_CONTENT}">                        <section class="txtArea">                            ${result.PAGE3_LEFT_CONTENT}                        </section>                        </c:if>                        <c:if test="${result.PAGE3_LEFT_KIND eq 'image' && not empty result.PAGE3_LEFT_IMG_FULL}">                        <section class="imgArea">                            <img src="${result.PAGE3_LEFT_IMG_FULL}">                        </section>                        </c:if>                        <c:if test="${result.PAGE3_RIGHT_KIND eq 'text' && not empty result.PAGE3_RIGHT_CONTENT}">                        <section class="txtArea">                            ${result.PAGE3_RIGHT_CONTENT}                        </section>                        </c:if>                        <c:if test="${result.PAGE3_RIGHT_KIND eq 'image' && not empty result.PAGE3_RIGHT_IMG_FULL}">                        <section class="imgArea">                            <img src="${result.PAGE3_RIGHT_IMG_FULL}">                        </section>                        </c:if>                    </li>                    </c:if>                </ul>        	</article>        </div>    </div>    <footer><!--         <div class="btns left"> --><!--             <button class="btn_pack btn_mo white" onclick="">수정</button> --><!--             <button class="btn_pack btn_mo white" onclick="">삭제</button> --><!--         </div> --><c:if test="${AUTHWRITER eq 'W' }">     	<div class="btns left">        	<button class="btn_pack btn_mo white" onclick="$('.btn_close').trigger('click');location.href='#/event/write.af?page=${result.PAGE_TYPE_TXT}&num=${resultParam.num}'">수정</button>        	<button class="btn_pack btn_mo white" onclick="doDelete()">삭제</button>        </div></c:if>        <div class="paging">            <span class="prev"></span><span class="curr">0</span> / <span class="count">0</span> <span class="next"></span>        </div>    </footer>    <script type="text/javascript">        var pageSlider = $('.page-slider').bxSlider({            slideSelector: '.page',            pager:false,            infiniteLoop: false,            controls: true,            nextSelector: '.paging .next',            prevSelector: '.paging .prev',            onSliderLoad: function(currentIndex){               $('.paging .curr').text(currentIndex+1);               $('.paging .count').text($('.page-slider .page').length);            },            onSlideBefore: function($slideElement, oldIndex, newIndex){               $('.paging .curr').text(newIndex+1);            },            onSlideAfter: function($slideElement, oldIndex, newIndex){                           }        });    </script>    <script type="text/javascript">	function doDelete(){		if(!confirm("삭제하시겠습니까? 복구될 수 없습니다.")) return;		$.ajax({			type:"POST",			url:"event/noticeEventDeleteAction.af",			dataType:"json",			data:{num:'${resultParam.num}'},			success : function(data) {				if(data.result){					alert(data.msg);					location.href="#/event/eventIndex.af?"+new Date().getTime();				}				else{					alert(data.msg);				}			},			error : function(xhr, status, error) {				alert("잠시 후에 다시하세요.");			}		});	}	</script></section>