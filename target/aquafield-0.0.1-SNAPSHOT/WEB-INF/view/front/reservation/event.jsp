<%@ page contentType="text/html; charset=utf-8" pageEncoding="utf-8"%><%@ include file="../../common/taglibs.jsp" %><button class="btn_close">닫기</button><div id="reservation_event" class="in inc_abs_foot_btn">    <c:choose>    	<c:when test="${!empty prodInfos}">		    <header class="hd_type2">		        <h2>이벤트 참여를 결정해주세요</h2>		    </header>		   	<form method="post" onsubmit="return false;">		    <section class="form_type_res">		      <c:forEach items="${prodInfos}" var="prodInfo" varStatus="status">		        <div class="limit_count_box" data-limit="${prodInfo.LIMIT}">		            <img class="ico" src="/common/front/images/ico/ico_res_limit.png"/>예약가능 인원 <span>${prodInfo.LIMIT}</span>명		        </div>		        <ul class="item">		            <li data-code="event${status.count}" data-title="${prodInfo.ITEM_NM}" data-uid="${prodInfo.SET_UID}">		                <input type="hidden" name="" class="price" value="${prodInfo.EVENT_PRICE}">		                <span class="tit">${prodInfo.ITEM_NM}</span>		                <input type="number" name="event${status.count}" class="iptNum resData">		                <div class="itmeTotal"></div>		            </li>		        </ul>			  </c:forEach>		    </section>		    </form>		    <footer class="btns center">		        <section class="form_type_res_total">		            <span class="tit">합계</span>		            <span class="price">0원</span>		            <input type="hidden" name="totalprice" class="totalprice resData">		        </section>		        <button onclick="reservationPop.setResData('item','.item li');" class="btn_pack wide btn_mo blue"><span>확인</span> </button>		    </footer>		    <script type="text/javascript">		        reservationPop.getResData('item','.item li');		        $(".iptNum").customNumber();		        $(".iptNum").change(function(event) {		            calculation(this);		        });		        calculation();		    </script>    	</c:when>    	<c:otherwise>	    <header class="hd_type2">	    	<h2>이벤트 참여를 결정해주세요</h2>	        <section class="form_type_res" style="text-align: center; padding-top: 100px;"><img class="ico" src="/common/front/images/ico/ico_none_event.png"/></section>	    </header>      	    	</c:otherwise>    </c:choose>		    </div>