<%@ page contentType="text/html; charset=utf-8" pageEncoding="utf-8"%><%@ include file="../../common/taglibs.jsp" %><button class="btn_close">닫기</button><div id="reservation_plant" class="in inc_abs_foot_btn">    <c:choose>    	<c:when test="${!empty prodInfos}">	    <header class="hd_type2">	        <h2>사용시설과 예약인원을 설정해주세요</h2>	    </header>    			   	<form method="post" onsubmit="return false;" class="myform">		    <section class="form_type_res">		        <div class="limit_count_box total" data-limit="10">		            <img class="ico" src="/common/front/images/ico/ico_res_limit.png"/>예약가능 인원 <span>10</span>명		        </div>		        <c:if test="${prodInfos[0] ne null && prodInfos[0] ne ''}">		        <div class="item">		            <h3>1.찜질스파</h3>		            <ul class="spa" data-limit="${prodInfos[0].QUANTITY}">		                <li data-code="spa1" data-title="${prodInfos[0].ITEM_NM}" data-age="(대인)" data-uid="${prodInfos[0].SET_UID}" >		                    <input type="hidden" name="price" class="price" value="${prodInfos[0].ADULTS_PRICE}">		                    <span class="tit">대 인 : </span>		                    <p><label>남</label>		                    <input type="number" name="spaman1" class="iptNum man"></p>		                    <p><label>여</label>		                    <input type="number" name="spawoman1" class="iptNum woman"></p>		                    <div class="itmeTotal"></div>		                </li>		                <li data-code="spa2" data-title="${prodInfos[0].ITEM_NM}" data-age="(소인)" data-uid="${prodInfos[0].SET_UID}">		                    <input type="hidden" name="price" class="price" value="${prodInfos[0].CHILD_PRICE}">		                    <span class="tit">소 인 : </span>		                    <p><label>남</label>		                    <input type="number" name="spaman2" class="iptNum man"></p>		                    <p><label>여</label>		                    <input type="number" name="spawoman2" class="iptNum woman"></p>		                    <div class="itmeTotal"></div>		                </li>		            </ul>		        </div>		        </c:if>		        <c:if test="${prodInfos[1] ne null && prodInfos[1] ne ''}">		        <div class="item">		            <h3>2.워터파크</h3>		            <ul class="water" data-limit="${prodInfos[1].QUANTITY}">	            		                <li data-code="weter1" data-title="${prodInfos[1].ITEM_NM}" data-age="(대인)" data-uid="${prodInfos[1].SET_UID}" >		                    <input type="hidden" name="price" class="price" value="${prodInfos[1].ADULTS_PRICE}">		                    <span class="tit">대 인 : </span>		                    <label>남</label>		                    <input type="number" name="waterman1" class="iptNum man">		                    <label>여</label>		                    <input type="number" name="waterwoman1" class="iptNum woman">		                    <div class="itmeTotal"></div>		                </li>		                <li data-code="weter2" data-title="${prodInfos[1].ITEM_NM}" data-age="(소인)" data-uid="${prodInfos[1].SET_UID}">		                    <input type="hidden" name="price" class="price" value="${prodInfos[1].CHILD_PRICE}">		                    <span class="tit">소 인 : </span>		                    <label>남</label>		                    <input type="number" name="waterman2" class="iptNum man">		                    <label>여</label>		                    <input type="number" name="waterwoman2" class="iptNum woman">		                    <div class="itmeTotal"></div>		                </li>		            </ul>		        </div>		        </c:if>		    </section>		    </form>		    <footer class="btns center">		        <section class="form_type_res_total">		            <span class="tit">합계</span>		            <span class="price">0원</span>		        </section>		        <button onclick="reservationPop.setResData('item','.item li');" class="btn_pack wide btn_mo blue"><span>확인</span> </button>		    </footer>		    <script type="text/javascript">		        reservationPop.getResData('item','.item li');		        $(".iptNum").customNumber();		        $(".iptNum").change(function(event) {		            calculation(this);		        });		        calculation();		    </script>    	    	</c:when>    	<c:otherwise>	    <header class="hd_type2">	        <h2>가격을 설정해주세요</h2>	    </header>      	    	</c:otherwise>    </c:choose></div>