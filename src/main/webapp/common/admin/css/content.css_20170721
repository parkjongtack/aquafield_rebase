@charset "utf-8";
html{font-size: 13px;color: #333;}
body{background-color: #eff0f5;}
#container{}
/*  로그인 */

#login .inner{width: 460px; margin:0 auto;margin-top: 5%;}
#login .loginBox{width: 100%;margin:0 auto; background-color: #fff;border: 1px solid #DADADA;box-shadow:0 1px 5px rgba(0,0,0,.2);}
#login .loginBox .loginHeader img { width: 100%; }
#login .loginBox .loginBody { padding: 50px; }
#login .loginBox h1{margin-bottom: 25px;}
#login .loginBox h1 span {font-size: 14px; line-height: 25px; color: #777; font-weight: lighter; }
#login .loginBox .inputBox { border-bottom: 1px solid #CBCBCB; padding: 10px;  }
#login .loginBox .inputBox > ul > li{margin-bottom:10px;}
#login .loginBox .inputBox > ul > li label{ width: 25%; height: 40px; display: inline-block; vertical-align: middle; line-height: 40px; box-sizing: border-box; }
#login .loginBox .inputBox > ul > li input{width:75%;height: 40px;border:1px solid #d9d9d9;padding:0 10px;border-radius:2px;color: #a3a3a3;font-size: 15px; box-sizing: border-box;text-align: left;}
#login .loginBox .inputBox > ul > li input.input_cert { width: 53%; }
#login .loginBox .inputBox > ul > li .btnBox { width: 20%; display: inline-block; padding: 0; vertical-align: middle;}
#login .loginBox .btnBox {position:relative; text-align: left;color: #666666;font-size: 14px;padding: 10px; }
#login .loginBox .btnBox .save_id { width: 48%; display: inline-block; box-sizing: border-box; vertical-align: middle; margin-top: 15px; }
#login .loginBox .btnBox .save_id > *{vertical-align: middle; display: inline-block;}
#login .loginBox .btnBox .save_id > input[type="checkbox"]{margin:0 5px 0 0;}
#login .loginBox .btnBox .btn_sign{display: inline-block;width: 50%;color: #fff;font-size: 16px;background-color: #33373f;height: 50px; line-height: 50px; font-weight: bold; box-sizing: border-box;}
#login .loginBox .btnBox .btn_cert { display: inline-block;color: #fff;font-size: 13px;background-color: #a1a1a1;width: auto; min-width: 70px; height: 40px; line-height: 40px; box-sizing: border-box; }
#login .loginFooter { padding: 15px 0; text-align: center; width: 100%;margin:0 auto; background-color: #fff;border: 1px solid #DADADA;box-shadow:0 1px 5px rgba(0,0,0,.2); margin-top:3px; }



/* 헤더 */

header{margin-bottom:30px;background:#eff0f5;}
header .gnb_area{height:74px;background:#fff;}
header .gnb_area>div{width:1180px;}
header .gnb_area>div>h1, header .gnb_area>div>div{float:left;}
header .gnb_area>div>h1{margin:10px 13px 0px 20px;}
header .gnb_area>div>h1 > span{margin-top:10px;font-family:'lato',sans-serif;font-weight:700;font-size:18px;color:#333333;vertical-align: middle;margin-left:10px;}

header #gnb{float:right;}
header #gnb .my_name{position:relative;float:left;margin-top:30px;color:#acacac;margin-right:20px;}
header #gnb .my_name .mylayer{position:absolute;z-index:1;left:-20px;display:none;
    top: 30px; min-width:174px; text-align: left; background: #fff; box-shadow: 0px 0px 5px 1px #a1a1a1; behavior: url('/oktomato/css/PIE.htc'); z-index: 1;}
header #gnb .my_name .mylayer:after {
    position: absolute;
    left: 35px;
    top: 0px;
    width: 10px;
    height: 10px;
    content: '';
    background: #fff;
    box-shadow: -2px -2px 5px -1px #a1a1a1;
    behavior: url('/oktomato/css/PIE.htc');
    -moz-transform: rotate(45deg);
    -o-transform: rotate(45deg);
    -webkit-transform: rotate(45deg);
    transform: rotate(45deg);
    -ms-filter: "progid:DXImageTransform.Microsoft.Matrix(M11=0.7071067811865474, M12=-0.7071067811865477, M21=0.7071067811865477, M22=0.7071067811865474, SizingMethod='auto expand')";
    filter: progid:DXImageTransform.Microsoft.Matrix( M11=0.7071067811865474, M12=-0.7071067811865477, M21=0.7071067811865477, M22=0.7071067811865474, SizingMethod='auto expand');
    margin-left: -5px;
    margin-top: -5px;
}

header #gnb .my_name .mylayer>ul{margin:15px 15px;}
header #gnb .my_name .mylayer>ul>li{padding:7px 0px;}
header #gnb .my_name .mylayer>ul>li >a {display:block;color:#333;}
header #gnb .my_name .mylayer>ul>li >a:hover{color:#5ca7fc;text-decoration:underline;}
header #gnb .my_name .mylayer>ul>li.last{border-top:1px dotted #ddd;padding-top:10px;margin-top:10px;}
header #gnb .my_name .name{color:#78a5ea;text-decoration:underline;}
header #gnb .my_name .btn_logout{color: #fff;font-size: 12px;background-color: #777777;padding:3px 13px;vertical-align: middle;margin-top:-3px;margin-left:5px;margin-right:10px;}

header #gnb .todolist{float:left;margin-top:25px; }
header #gnb .todolist > h2{color:#383838;font-size:13px;float:left;margin-top:5px;}
header #gnb .todolist > ul{position:relative;float: left;border-left:1px solid #E6E6E6;margin-left:12px;padding-left:2px;}
header #gnb .todolist > ul > li{display:inline-block;vertical-align: middle;margin:0px 10px;}
header #gnb .todolist > ul > li > a{position:relative;display:block;}
header #gnb .todolist > ul > li > a > .alert{position:absolute;display:block;bottom:-5px;right:-8px;background:#fc5c5c;color:#fff;font-size:11px;text-align:center;padding:1px 4px;border-radius:8px;}
header #gnb .todolist > ul > li > a > .alert2 { color: 000; font-weight: bold; margin-left: 5px; }
header #gnb .todolist > ul > li .alert_mg{position:absolute;padding-top:5px;left:10px;display:none;z-index:1;}
header #gnb .todolist > ul > li .alert_mg > div{padding:10px;font-size:12px;color:#acacac;background:#000;rgba(0,0,0,0.9);max-width:120px;line-height:16px;}
header #gnb .todolist > ul > li .alert_mg > div > a{color:#fcba5c;}
header #gnb .todolist > ul > li .alert_mg > div > a > img{margin-bottom:2px;}
header #gnb .todolist > ul > li:hover .alert_mg{display:block;}


/* 푸터 */
#footer {background:#595959;color: #fff; width: 100%; }
#footer .inner { height: auto; overflow: hidden; max-width: 1180px; padding: 20px; box-sizing: border-box; }
#footer .copyright { float: left; font-size: 15px; vertical-align: middle; margin-top: 10px; }
#footer .logo { background-color: #fff; display: inline-block; padding: 5px 10px; float: right; }
#footer .logo img { width: auto; }
#footer.fixed { position: fixed; bottom: 0; left: 0; }


/*LNB area*/
header #lnb{position: relative;overflow:hidden;}
header #lnb>ul{list-style:none;font-size:15px;background:#2d353c;border-bottom:3px solid #21272c;}
header #lnb>ul>li{position:relative;text-align:center;float:left;margin-bottom:-3px;}
header #lnb>ul>li>a{display:block;padding:14px 38px;color:#ffffff;}
header #lnb>ul>li.on:after { content: ''; width: 0; height: 0; border-left: 6px solid transparent; border-right: 6px solid transparent; border-bottom: 7px solid #eff0f5; position: absolute; bottom: 0; left: 50%; margin-left: -6px; }
#menu_arrow{position:absolute;bottom:-10px;}
#menu_arrow>img{display:block;}

/*LEFT area*/
#area_left_menu{position:absolute;width:200px;background:#eff0f5;overflow:hidden;left:0px;}

#area_left_menu .lnb_depth2{position:relative;}
#area_left_menu ul.lnb_depth2>li{position:relative;line-height:38px;padding:0px 20px 0px 0px;overflow:hidden;margin-bottom:10px;}
#area_left_menu ul.lnb_depth2>li>a{position:relative;display:block;padding:8px 15px 8px 18px;border-radius:0px 3px 3px 0px;
    -webkit-transition: .1s linear;  -moz-transition: .1s linear;  -o-transition: .1s linear;  transition: .1s linear;
}
#area_left_menu ul.lnb_depth2>li>a:before{position:absolute;top:0px;left:-4px;height:100%;width:4px; background-color:#183666; content:'';opacity:0;
    -webkit-transition: .1s linear;  -moz-transition: .1s linear;  -o-transition: .1s linear;  transition: .1s linear;
}
#area_left_menu ul.lnb_depth2>li>a:after{display:block;content:"";clear:both;
}
#area_left_menu ul.lnb_depth2>li>a>span.menu_tit{display:block;font-size:15px;color:#888888;line-height:22px;
-webkit-transition: .1s linear;  -moz-transition: .1s linear;  -o-transition: .1s linear;  transition: .1s linear;
}
#area_left_menu ul.lnb_depth2>li>a>span.right_arrow{position:absolute;display:block;height:100%;visibility:hidden;top:0px;right:8px;opacity:0;
-webkit-transition: .1s linear;  -moz-transition: .1s linear;  -o-transition: .1s linear;  transition: .1s linear;
}
#area_left_menu ul.lnb_depth2>li>a>span.right_arrow>img{vertical-align:middle;}

#area_left_menu ul.lnb_depth2>li>a:hover{background:#214c8f;}
#area_left_menu ul.lnb_depth2>li>a:hover>span.menu_tit{color:#fff;}
#area_left_menu ul.lnb_depth2>li>a:hover>span.right_arrow{visibility:visible;opacity:1;}
#area_left_menu ul.lnb_depth2>li>a:hover:before{left:0px;opacity:1;}

#area_left_menu ul.lnb_depth2>li.on>a{background:#214c8f;}
#area_left_menu ul.lnb_depth2>li.on>a>span.menu_tit{color:#fff;}
#area_left_menu ul.lnb_depth2>li.on>a>span.right_arrow{visibility:visible;opacity:1;}
#area_left_menu ul.lnb_depth2>li.on>a:before{left:0px;opacity:1;}


/*contents area*/
#contents{padding-left:200px;top:0px;z-index:3;padding-bottom:80px;/* margin:0px auto; */min-width:980px;max-width:980px;}
#contents.wide{max-width:1200px; padding-left: 20px;}

/*contents_bx area*/
.contents_bx_type1{border:1px solid #e3e3e3;background:#fff;margin-bottom:80px; box-sizing: border-box;}
.contents_bx_inner{margin: 20px 20px 40px 20px;position: relative;}


/*aside*/
#area_aside{float: left;margin-left:28px;}
#area_aside > .inner{position: fixed;}
#btns_create_survey{width: 268px;padding: 10px;border: 2px solid #7BA0C2;background-color: #f7f8fa;border-radius:4px;box-shadow:2px 2px 3px rgba(0,0,0,0.2);}
#btns_create_survey > dl{}
#btns_create_survey > dl > dt{margin-bottom: 14px;}
#btns_create_survey > dl > dt:only-child{margin-bottom: 0;}
#btns_create_survey > dl > dd{margin-bottom: 3px;}
#btns_create_survey > dl div{width: 100%;height: 37px;font-size: 15px;padding: 0 18px;text-align: left;border-radius:3px;position: relative;font-weight: 700;box-sizing:border-box;margin:0;}
#btns_create_survey > dl .ico_plus{position: absolute;width:14px;height:14px;top: 0;bottom: 0;right:18px;margin: auto;display: block;
-webkit-transition: -webkit-transform .3s ease-in-out; -moz-transition: -moz-transform .3s ease-in-out; -ms-transition: -ms-transform .3s ease-in-out; -o-transition: -o-transform .3s ease-in-out; transition: transform .3s ease-in-out;
-webkit-transform: rotate(0deg); -moz-transform: rotate(0deg); -ms-transform: rotate(0deg); -o-transform: rotate(0deg); transform: rotate(0deg);}
#btns_create_survey > dl .ico_plus > span{position: absolute;height:2px;width:14px;background-color: #1772C5;display: block;top: 0;bottom: 0;right: 0;left: 0;margin: auto;}
#btns_create_survey > dl .ico_plus > span:after{position: absolute;width: 2px;height: 14px;background-color: #1772C5;display: block;top: 0;bottom: 0;right: 0;left: 0;margin: auto;content: '';}
#btns_create_survey > dl > dt > div{background-color: #fff;color: #277fd0;border: 1px solid #1772C5;line-height: 35px;}
#btns_create_survey > dl > dd > div{background-color: #348fe2;color: #fff;line-height: 37px;cursor: move;}
#btns_create_survey > dl > dd > div.ui-draggable-dragging{z-index: 1;}

#btns_create_survey > dl button{width: 100%;height: 37px;font-size: 15px;padding: 0 18px;text-align: left;border-radius:3px;position: relative;font-weight: 700;box-sizing:border-box;margin:0;}
#btns_create_survey > dl > dt > button:hover .ico_plus{ -webkit-transform: rotate(180deg); -moz-transform: rotate(180deg); -ms-transform: rotate(180deg); -o-transform: rotate(180deg); transform: rotate(180deg); }
#btns_create_survey > dl > dt > button{background-color: #fff;color: #277fd0;border: 1px solid #1772C5;line-height: 35px;}
#btns_create_survey > dl > dd > button{background-color: #348fe2;color: #fff;line-height: 37px;}
#btns_create_survey > dl > dd > button.off{background-color: #acacac;}

/*common tip*/
