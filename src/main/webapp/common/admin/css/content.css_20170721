/*테이블 스타일*/
.tb_type1{}
.tb_type1 > table{table-layout: fixed;width: 98.5%;border:1px solid #E2E7EB;margin-left: 1.5%;}
.tb_type1.no_fx > table{table-layout:auto;}
.tb_type1 th, .tb_type1 td{padding:8px 20px;} 
.tb_type1 > table > thead > tr > th, 
.tb_type1 > table > tbody > tr > th, 
.tb_type1 > table > tbody > tr > td {border-top:1px solid #E2E7EB;vertical-align:middle;padding:8px 7px;border-left:1px solid #E2E7EB; font-size: 13px;}
.tb_type1 > table > thead > tr > th{font-weight: 700;background-color: #f9fbfb;border-bottom:1px solid #E2E7EB;line-height: 20px;}
.tb_type1 > table > thead > tr > th.status{text-align: right;}
.tb_type1 > table > thead > tr > th.btns{text-align: right;}
.tb_type1 > table > thead > tr > th.btns button{margin: -2px 0; height: 24px; background-color: transparent; color: #666; text-decoration: underline; padding: 0;box-shadow:none;}
.tb_type1 > table > tbody > tr > th.left { text-align: left; }
.tb_type1 > table > tbody > tr > th.right { text-align: right; }
.tb_type1 > table > tbody > tr > th{font-weight: 700;-ms-text-overflow: ellipsis; text-overflow: ellipsis;overflow: hidden;white-space: nowrap;border-bottom:1px solid #E2E7EB;text-align: center;line-height: 20px;background-color: #f9fbfb;}
.tb_type1 > table > tbody > tr > td{color: #666666; text-align:center;-ms-text-overflow: ellipsis; text-overflow: ellipsis;overflow: hidden;white-space: nowrap;border-bottom:1px solid #E2E7EB;line-height: 20px;}
.tb_type1 > table > tbody > tr > td.tit { text-align: left; }
.tb_type1 > table > tbody > tr > td.left { text-align: left; }
.tb_type1 > table > tbody > tr > td.right { text-align: right; }
.tb_type1.no_fx > table > tbody > tr > th, .tb_type1.no_fx > table > tbody > tr > td{white-space: normal;}
.tb_type1.end > table > thead > tr > th{background-color: #f2f7f7;}
.tb_type1.end > table > thead > tr > th.status{color: #9b9b9b;}
.tb_type1.ing > table > thead > tr > th{background-color: #eff5fa;}
.tb_type1.ing > table > thead > tr > th.status{color: #5ca7fc;}
.tb_type1 > table > tbody > tr > td.cont_sms{white-space: normal;border-left:1px solid #E2E7EB;}
.tb_type1 > table > tbody > tr > td.res { color: #00a651; }
.tb_type1 > table > tbody > tr > td.cancel { color: #fc5c5c; }
.tb_type1 > table tr > td.price { text-align: right; }
.tb_type1.mb20 { margin-bottom: 20px; }

.tb_type2 > table{width: 98.5%;border:1px solid #E2E7EB; margin-left: 1.5%;}
.tb_type2 > table > tbody > tr > th, .tb_type2 > table > tbody > tr > td{vertical-align: middle;}
.tb_type2 > table > tbody > tr > th{font-weight: 700;white-space: nowrap;border-bottom:1px solid #E2E7EB;padding: 10px 20px;width:120px;border-left: 1px solid #E2E7EB; border-right:1px solid #E2E7EB;text-align: left;background-color: #f9fbfb;font-size: 14px;}
.tb_type2 > table > tbody > tr > th:frist-child { border-left: 0; }
.tb_type2 > table > tbody > tr > td{color: #666666;border-bottom:1px solid #E2E7EB;padding: 6px 15px;}
