/*섹션 스타일*/
#container .sect_type1{margin-bottom: 25px;}
.sect_type1 > h2{font-size: 16px;color: #115fb0;margin-bottom:13px;}
.sect_type1 > .bx_cont{padding:20px 19px 50px 19px;border-radius:4px;background-color: #fff;box-shadow:1px 1px 5px -2px rgba(0,0,0,0.3)}

.sect_type1 > .bx_cont.type1{padding: 20px 19px;margin-bottom:11px;}
.sect_type1 > .bx_cont.type1 > h3{margin-bottom: 24px;}
.sect_type1 > .bx_cont.type1 .form_type1{padding: 0 24px;}
.sect_type1 > .bx_cont.type1 > h3.on{}
.sect_type1 > .bx_cont.type1 > h3 ~ .bot_btns button{opacity:0.5; -webkit-transition: opacity .3s linear; -moz-transition: opacity .3s linear; -ms-transition: opacity .3s linear; -o-transition: opacity .3s linear; transition: opacity .3s linear;}
.sect_type1 > .bx_cont.type1 > h3.on ~ .bot_btns button{opacity:1;}
.sect_type1 > .bx_cont.type1 > h3 + .form_type1 > ul > li > strong{color: #999;-webkit-transition: color .3s linear; -moz-transition: color .3s linear; -ms-transition: color .3s linear; -o-transition: color .3s linear; transition: color .3s linear;}
.sect_type1 > .bx_cont.type1 > h3.on + .form_type1 > ul > li > strong{color: #333;}

.sect_type1 > .bx_cont.type2{padding: 0;border: 1px solid #BEBEBE;margin-bottom:18px;position: relative;}
.sect_type1 > .bx_cont.type2 > header{padding:13px 25px;background-color: #333;}
.sect_type1 > .bx_cont.type2 > header:after{content: '';display: table;clear: both;}
.sect_type1 > .bx_cont.type2 > header > h3{color: #fff;font-size: 15px;line-height: 20px;float: left;font-weight: 400;}
.sect_type1 > .bx_cont.type2 > header > span.status{float: right;display: block;line-height: 20px;font-size: 15px;color: #2fb1f3;}
.sect_type1 > .bx_cont.type2 > article{padding: 34px 19px;}
.sect_type1 > .bx_cont.type2 .btn_search{margin-top:10px;text-align: right;}


/*리스트 스타일*/
