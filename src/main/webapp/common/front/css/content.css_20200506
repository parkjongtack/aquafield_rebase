.paraShowing{opacity: 0;}
.wordBraek{-ms-word-break: keep-all;word-break: keep-all;}
/* MAIN */
#main #main-slider { position: relative; }
#main #main-slider .slider-item { width: 100%; }
#main #main-slider .slider-item img { display: inline-block; }
#main #main-slider .slider-item.main01 { background: url("../images/main/main_bg_01.jpg") center no-repeat; background-size: cover; }
#main #main-slider .slider-item.main02 { background: url("../images/main/main_bg_02.jpg") center no-repeat; background-size: cover; }
#main #main-slider .slider-item.main03 { background: url("../images/POINT03/main/main_bg_03.jpg") center no-repeat; background-size: cover; }
#main #main-slider .slider-item.main04 { background: url("../images/POINT03/main/main_bg_04.jpg") center no-repeat; background-size: cover; }
#main #main-slider .sliderInner { width: 100%; height: 100%; display: table }
#main #main-slider .textArea { display: table-cell; vertical-align: middle; text-align: center; font-size: 1.384rem; color: #f7f8f9; font-family: 'Playfair Display', serif; text-shadow: 2px 0px 30px #000; padding: 40px; }
#main #main-slider .textArea h1 {
    font-size: 6.923rem;
    line-height: 8rem;
    display: block;
    font-weight: lighter;
}
#main .textArea p {
    font-size: 2.69rem;
    line-height: 8rem;
    display: block;
    font-weight: lighter;
}

#main #main-slider ul.slick-dots { position: absolute; z-index: 55; text-align: center; bottom:15%; left: 0; width: 100%; }
#main #main-slider .bx-pager { display: inline-block; }
#main #main-slider ul.slick-dots li { display: inline-block; }
#main #main-slider ul.slick-dots li button {
    width: 13px;
    height: 13px;
    margin: 7.5px 7.55px;
    background: #fff;
    display: inline-block;
    -webkit-backface-visibility: visible;
    -webkit-transition: all 200ms ease;
    -moz-transition: all 200ms ease;
    -ms-transition: all 200ms ease;
    -o-transition: all 200ms ease;
    transition: all 200ms ease;
    -webkit-border-radius: 30px;
    -moz-border-radius: 30px;
    border-radius: 30px;
    opacity: .5;
    -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=50)"; /* IE 8 */
    filter: progid:DXImageTransform.Microsoft.Alpha(Opacity=50); /* IE 7 and olders */
    text-indent: -9999px;
}
#main #main-slider ul.slick-dots li.slick-active button { width: 37px; }
#main #main-slider ul.slick-dots li.slick-active button,
#main #main-slider ul.slick-dots li button:hover {
    opacity: .7;
    -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=70)"; /* IE 8 */
    filter: progid:DXImageTransform.Microsoft.Alpha(Opacity=70); /* IE 7 and olders */
}

#main #main-slider .slick-prev { position: absolute; left: 0; top: 50%; text-indent: -9999px; width: 40px; height: 74px; margin-top: -37px;  background: url(../images/main/main_silder_prev.png) center no-repeat; z-index: 1; display: none !important;}
#main #main-slider .slick-next { position: absolute; right: 0; top: 50%; text-indent: -9999px; width: 40px; height: 74px; margin-top: -37px; background: url(../images/main/main_silder_next.png) center no-repeat; z-index: 1; display: none !important;}

#main #main-slider ul.slick-dots li button.slick-stop {
    width: 8px;
    height: 13px;
    background: url(../images/ico/ico_player.png) 0 0 no-repeat;
    display: inline-block;
    margin: 7.5px 7.55px;
    cursor: pointer;
    opacity: .7;
    -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=70)"; /* IE 8 */
    filter: progid:DXImageTransform.Microsoft.Alpha(Opacity=70); /* IE 7 and olders */
    background-size: cover;
    text-indent: -9999px;
    border-radius: 0;
}
#main #main-slider ul.slick-dots li button.slick-start {
    width: 11px;
    height: 13px;
    background: url(../images/ico/ico_player.png) -17px 0 no-repeat;
    display: inline-block;
    margin: 7.5px 7.55px;
    cursor: pointer;
    opacity: .7;
    -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=70)"; /* IE 8 */
    filter: progid:DXImageTransform.Microsoft.Alpha(Opacity=70); /* IE 7 and olders */
    background-size: cover;
    text-indent: -9999px;
    border-radius: 0;
}
#main #main-slider ul.slick-dots li button.slick-stop:hover,
#main #main-slider ul.slick-dots li button.slick-start:hover,
#main #main-slider ul.slick-dots li button.slick-stop.active,
#main #main-slider ul.slick-dots li button.slick-start.active { opacity: 1;
    -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=100)"; /* IE 8 */
    filter: progid:DXImageTransform.Microsoft.Alpha(Opacity=100); /* IE 7 and olders */
}



#header {  }
