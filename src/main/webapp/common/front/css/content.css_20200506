#content_change_btns{height: 0;width:1350px;max-width: 100%;position: absolute;top: 0;bottom: 0;left: 0;right: 0;margin: auto;z-index: 5;}
#content_change_btns button{width: 80px;height: 80px;top: 0;bottom: 0;margin: auto;background-color: #343434;position: absolute;box-shadow: 3px 3px 5px -1px rgba(0,0,0,0.4);
    text-indent: -9999px;opacity: 1;visibility: visible;
    -webkit-backface-visibility: hidden;
	-moz-backface-visibility: hidden;
	-ms-backface-visibility: hidden;
	backface-visibility: hidden;

    -webkit-transform: rotateY(0deg) translateZ(0px);
    -moz-transform: rotateY(0deg) translateZ(0px);
    -o-transform: rotateY(0deg) translateZ(0px);
    -ms-transform: rotateY(0deg) translateZ(0px);
    transform: rotateY(0deg) translateZ(0px);
    -webkit-transform-style: preserve-3d;
    -moz-transform-style: preserve-3d;
    -o-transform-style: preserve-3d;
    -ms-transform-style: preserve-3d;
    transform-style: preserve-3d;
    -webkit-transition:visibility .4s ease-in-out, opacity .4s ease-in-out, -webkit-transform .4s cubic-bezier(.67,.04,.32,1);
    -moz-transition:visibility .4s ease-in-out, opacity .4s ease-in-out, -moz-transform .4s cubic-bezier(.67,.04,.32,1);
    -ms-transition:visibility .4s ease-in-out, opacity .4s ease-in-out, -ms-transform .4s cubic-bezier(.67,.04,.32,1);
    -o-transition:visibility .4s ease-in-out, opacity .4s ease-in-out, -o-transform .4s cubic-bezier(.67,.04,.32,1);
    transition:visibility .4s ease-in-out, opacity .4s ease-in-out, transform .4s cubic-bezier(.67,.04,.32,1);
}
#content_change_btns button.btn_prev{left: -55px;}
#content_change_btns button.btn_next{right: -55px;}
#content_change_btns button:before,
#content_change_btns button:after{position:absolute;content: ''; top:0;left:0;bottom:0;right:0;margin:auto;width:20px;height:20px;
    -webkit-transition: -webkit-transform .4s cubic-bezier(.67,.04,.32,1);
    -moz-transition: -moz-transform .4s cubic-bezier(.67,.04,.32,1);
    -ms-transition: -ms-transform .4s cubic-bezier(.67,.04,.32,1);
    transition: transform .4s cubic-bezier(.67,.04,.32,1);
}
#content_change_btns button:before{ms-transform: rotate(-45deg); /* IE 9 */    -webkit-transform: rotate(-45deg); /* Safari */    transform: rotate(-45deg);}
#content_change_btns button:after{ms-transform: rotate(45deg); /* IE 9 */    -webkit-transform: rotate(45deg); /* Safari */    transform: rotate(45deg);}

#content_change_btns button.btn_prev:before{border-top:2px solid #fff;left: 10px;}
#content_change_btns button.btn_prev:after{border-bottom:2px solid #fff;left: 10px;}
#content_change_btns button.btn_next:before{border-bottom:2px solid #fff;right: 10px;}
#content_change_btns button.btn_next:after{border-top:2px solid #fff;right: 10px;}
#content_change_btns.hide button{opacity: 0;visibility: hidden;}

#content_change_btns.hide button.btn_prev{
    -webkit-transform: rotateY(-90deg);
    -moz-transform: rotateY(-90deg);
    -o-transform: rotateY(-90deg);
    -ms-transform: rotateY(-90deg);
    transform: rotateY(-90deg);
}
#content_change_btns.hide button.btn_next{
    -webkit-transform: rotateY(90deg);
    -moz-transform: rotateY(90deg);
    -o-transform: rotateY(90deg);
    -ms-transform: rotateY(90deg);
    transform: rotateY(90deg);
}

#content_change_btns button:hover:before{ms-transform: rotate(225deg); -webkit-transform: rotate(225deg); transform: rotate(225deg);}
#content_change_btns button:hover:after{ms-transform: rotate(135deg); -webkit-transform: rotate(135deg); transform: rotate(135deg);}


