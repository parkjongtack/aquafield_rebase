#mainMenu { position: absolute; bottom: 40px; left: 0; width: 100%; height: auto; z-index: 60; font-family: 'Noto Sans KR', sans-serif; font-weight: lighter; }
#mainMenu > .inner { position: relative; }
#mainMenu .navArea { background-color: #102230;  }
#mainMenu ul.nav { display: table; width: 100%; height: 60px; max-width: 1350px; margin: 0 auto; }
#mainMenu ul.nav > li { display: table-cell; width: 25%; text-align: center; vertical-align: middle; }
#mainMenu ul.nav > li > a { display: block; height: 60px; line-height: 60px; position: relative; z-index: 1; color: #e3ddbf; font-size: 20px; font-weight: lighter; -webkit-transition: all .5s; transition: all .5s; overflow: hidden; }
#mainMenu ul.nav > li > a:hover { color: #fff; }
#mainMenu ul.nav > li > a.active { color: #efe7c3; }
#mainMenu ul.nav > li > a:after { content: ""; position: absolute; z-index: -1; display: block; top: 0; left: 0; width: 100%; height: 100%; background: #1d394e;
    -webkit-transform-origin: top left;
    transform-origin: top left;
    -webkit-transform: scaleY(0);
    transform: scaleY(0);
    -webkit-transition: all .7s;
    transition: all .7s;
}
#mainMenu ul.nav > li > a:hover:after {
    -webkit-transform: scaleY(0.15);
    transform: scaleY(0.15);
}
#mainMenu ul.nav > li > a.active:after {
    -webkit-transform: scaleY(1);
    transform: scaleY(1);
}

/* lang */
#mainMenu ul.nav > li > a .lang_en { display: block; position: absolute; top: 0; left: 0; width: 100%; }
#mainMenu ul.nav > li > a .lang_ko { display: none; }
#mainMenu ul.nav > li > a:hover .lang_en { display: none; }
#mainMenu ul.nav > li > a:hover .lang_ko  { display: block; }

#mainMenu ul.nav > li > .hideMenu { width: 100%; height: 0; background-color: #3d3c39; overflow: hidden; position: absolute; bottom:0; left: 0; margin-bottom: 60px; display: none; z-index: 95; }
#mainMenu ul.nav > li > .hideMenu .inner {overflow: hidden; background:url("../images/common/transparent.png") repeat; box-sizing: border-box; height: 100%; position: relative; max-width: 1350px; margin: 0 auto; padding: 25px 60px; }
#mainMenu ul.nav > li > .hideMenu ul { float: left; text-align: left; }
#mainMenu ul.nav > li > .hideMenu ul li {  }
#mainMenu ul.nav > li > .hideMenu ul li a { color: #968776; -webkit-transition: all .5s; transition: all .5s; display: block; font-size: 15px; line-height: 35px; font-family: 'Playfair Display', serif; position: relative; }
#mainMenu ul.nav > li > .hideMenu ul li a:hover { color: #fff; padding-left: 20px; }
#mainMenu ul.nav > li > .hideMenu ul li a:hover span { text-decoration: underline; }
#mainMenu ul.nav > li > .hideMenu ul li a:hover:before { content: '>'; position: absolute; left: 4px; }

#mainMenu #menuSlider { width: 100%; height: 0; background-color: rgba(29,57,78,0.8); overflow: hidden;}
#mainMenu #menuSlider .slick-list { position: relative; max-width: 1350px; margin: 0 auto; box-sizing: border-box; overflow: hidden;}
#mainMenu #menuSlider .menuItem { height: 255px !important; outline: none !important; padding: 25px 60px;box-sizing: border-box;}
#mainMenu #menuSlider .menuItem .inner {overflow: visible; background:url("../images/common/transparent.png") repeat;  height: 100%; position: relative; max-width: 1350px; margin: 0 auto;  }
#mainMenu #menuSlider .menuItem ul.menu_list { float: left; text-align: left; width: 190px; }
#mainMenu #menuSlider .menuItem ul.menu_list > li {  }
