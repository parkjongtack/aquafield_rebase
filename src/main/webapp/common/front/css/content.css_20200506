    -webkit-transition: color .5s ease, padding-left .5s ease;
    -o-transition: color .5s ease, padding-left .5s ease;
    transition: color .5s ease, padding-left .5s ease;
}
#mainMenu #menuSlider .menuItem ul.menu_list > li a span.lang_en { display: inline-block; font-family: 'Playfair Display', serif; }
#mainMenu #menuSlider .menuItem ul.menu_list > li a span.lang_ko { display: none; }
