#mainMenu #menuSlider .menuItem ul.menu_list > li a:hover span.lang_en { display: none; }
#mainMenu #menuSlider .menuItem ul.menu_list > li a:hover span.lang_ko { display: inline-block; }
#mainMenu #menuSlider .menuItem ul.menu_list > li a:before { content: ''; position: absolute; left: 4px; top: 50%; width: 5px; height: 9px; margin-top: -4.5px; background:url("../images/ico/ico_menu_arrow.png") center no-repeat; opacity: 0;
    -webkit-transition: opacity .5s ease-in-out, -webkit-transform .3s ease-in-out;
    -moz-transition: opacity .5s ease-in-out, -moz-transform .3s ease-in-out;
    -ms-transition: opacity .5s ease-in-out, -ms-transform .3s ease-in-out;
    -o-transition: opacity .5s ease-in-out, -o-transform .3s ease-in-out;
}
#mainMenu #menuSlider .menuItem ul.menu_list > li a:hover:before { opacity: 1; }


#mainMenu #menuSlider .menuItem ul.menu_content { max-width: 800px; overflow: visible; padding-right: 12%; position: absolute;left: 25%;right: 0;top: 0;bottom: 0; z-index: 1;}
#mainMenu #menuSlider .menuItem ul.menu_content > li { position: absolute;height: 100%;left: 0;right: 0;top: 0;bottom: 0;opacity: 0;visibility: hidden;
    -webkit-transform: translateY(50px);
    -ms-transform: translateY(50px);
    -o-transform: translateY(50px);
    transform: translateY(50px);
    -webkit-transition: opacity .3s ease-in-out, visibility .3s ease-in-out, -webkit-transform .5s cubic-bezier(1,-1.47,1,1);
    -o-transition: opacity .3s ease-in-out, visibility .3s ease-in-out, -o-transform .5s cubic-bezier(1,-1.47,1,1);
    transition: opacity .3s ease-in-out, visibility .3s ease-in-out, transform .5s cubic-bezier(1,-1.47,1,1);
}
#mainMenu #menuSlider .menuItem ul.menu_content > li.on{opacity: 1;visibility: visible;z-index: 1;
    -webkit-transform: translateY(0px);
    -ms-transform: translateY(0px);
    -o-transform: translateY(0px);
    transform: translateY(0px);
    -webkit-transition: opacity .3s ease-in-out, visibility .3s ease-in-out, -webkit-transform .3s ease-out;
    -o-transition: opacity .3s ease-in-out, visibility .3s ease-in-out, -o-transform .3s ease-out;
    transition: opacity .3s ease-in-out, visibility .3s ease-in-out, transform .3s ease-out;
}
#mainMenu #menuSlider .menuItem ul.menu_content .img { width: 50%; float: left; box-sizing: border-box; padding: 0 20px; text-align: center; }
#mainMenu #menuSlider .menuItem ul.menu_content .img img { display: inline-block; max-width: 100%; }
