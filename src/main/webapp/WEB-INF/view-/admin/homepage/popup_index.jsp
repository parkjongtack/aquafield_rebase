<%@ page contentType="text/html; charset=utf-8" pageEncoding="utf-8"%><%@ include file="../../common/taglibs.jsp" %><script type="text/javascript">	var page = 1;		$( document ).ready(function() {			});	//검색	function doSearch(){		$("input[name='page']").val("1");		page = 1;		getData();	};		var goWrite = function(){		var url = "/secu_admaf/admin/homepage/popup_regist.af";		$("#gotoUrl").attr('action',url);		$("#gotoUrl").submit();		}</script><section id="contents">	<div class="contents_bx_type1">		<div class="contents_bx_inner">			<div id="path" class="fixed">				<div class="pathInner">					<h2>팝업관리</h2>					<div class="btns_area"> 						<a href="#write" class="btn_pack btn_mo d_gray" onclick="goWrite();return false;"><img src="/admaf/images/common/ico_add.png">등록</a>					</div>				</div>			</div>			<form name="search_form" id="search_form" action="" method="post" onsubmit="doSearch();return false;">				<div class="tb_type2">					<h3>조건검색</h3>					<table>						<tbody>							<tr>								<th>검색조건</th>								<td colspan="3">									<select name="srch_type" id="srch_type" class="customized-select">										<option value="title" <c:if test="${resultParam.reserve_state == code.CODE_ID }">selected="selected"</c:if> >제목</option>										<option value="content" <c:if test="${resultParam.reserve_state == code.CODE_ID }">selected="selected"</c:if> >내용</option>									</select>									<input type="text" name="TERMS_TITLE" class="ipt2 w562mx100" value="${result.TERMS_TITLE}" />								</td>							</tr>							<tr>								<th colspan="4">									<div class="tb_btn_area center">										<button class="btn_pack btn_mo d_gray" type="button" onclick="doSearch();">Search</button>									</div>								</th>							</tr>						</tbody>					</table>				</div>				<input type="hidden" name="page" value="${page }">			</form>			<form name="gotoUrl" id="gotoUrl" method="post">			</form>			<div class="tb_type1">				<h3>검색결과</h3>				<div class="result_summary">Total : <strong>${totCnt }</strong></div>				<table>				 	<colgroup> 						<col width="6%"/> 						<col width="5%"/> 						<col width="9%"/> 						<col width="10%"/> 						<col width="4%"/> 					</colgroup>				 	<thead>				 		<tr>							<th>No.</th>							<th>제목</th>							<th>첨부파일</th>							<th>등록자</th>							<th>등록일</th>						</tr>				 	</thead>					<tbody>						<c:choose>							<c:when test="${empty pList }">								<tr>									<td colspan="5"> 조회된 내용이 없습니다. </td>								<tr>							</c:when>							<c:otherwise>								<c:forEach items="${pList }" var="pList">									<tr>										<td>${pList.RN }</td>										<td><a href="/secu_admaf/admin/homepage/popupModify.do?popupSeq=${pList.POPUP_SEQ }">${pList.TITLE }</a></td>										<td>${pList.rn }</td>										<td>${pList.rn }</td>										<td>${pList.INS_DATE }</td>									</tr>								</c:forEach>							</c:otherwise>						</c:choose>					</tbody>				</table>			</div>			<div class="paginate">				<c:if test="${pu.blockStartNum > 1 }">					<a href="javascript:;" onclick="doPaging('${pu.blockStartNum - 1 }');" class="btn prev disable">PREV</a>				</c:if>				<c:forEach var="i" begin="${pu.blockStartNum}" end="${pu.blockEndNum }">					<a href="javascript:;" onclick="doPaging('${i}');" class="num <c:if test="${i == pu.pageNum }">on</c:if>">${i}</a>				</c:forEach>				<c:if test="${pu.blockEndNum < pu.pageTotalCount }">					<a href="javascript:;" onclick="doPaging('${pu.blockEndNum + 1 }');" class="btn next">NEXT</a>				</c:if>			</div>		</div>	</div></section>