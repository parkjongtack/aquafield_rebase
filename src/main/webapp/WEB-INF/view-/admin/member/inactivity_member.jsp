<%@ page contentType="text/html; charset=utf-8" pageEncoding="utf-8"%><%@ include file="../../common/taglibs.jsp" %>
<section id="contents">	<div class="contents_bx_type1">		<div class="contents_bx_inner">			<div id="path" class="fixed">				<div class="pathInner">					<h2>휴면계정관리</h2>				</div>			</div>			<form name="serach_form" action="/secu_admaf/admin/member/inactivity.af" method="post" onsubmit="retrun false;">				<input type="hidden" name="order_res" value="${resultParam.order_res }">				<input type="hidden" name="page" value="${resultParam.page }">					<div class="tb_type2">					<h3>조건검색</h3>					<table>						<tbody>							<tr>								<th>회원정보</th>								<td><input type="text" name="srch_text" value="${resultParam.srch_text }" class="ipt2 w100p"/></td>								<th>상	태</th>								<td>									<select name="srch_status" id="srch_status" class="customized-select">										<option value="">전체</option>										<option value="N" <c:if test="${resultParam.srch_status eq 'N' }">selected="selected"</c:if>>대기</option>										<option value="Y" <c:if test="${resultParam.srch_status eq 'Y' }">selected="selected"</c:if>>휴면</option>									</select>								</td>							</tr>							<tr>								<th>일자검색</th>${resultParam.LAST_LOGIN_DATE}								<td colspan="3">									<select name="srch_reg_type" id="srch_reg_type" class="customized-select">										<option value="">전체</option>										<option value="LAST_LOGIN_DATE" <c:if test="${resultParam.srch_reg_type eq 'LAST_LOGIN_DATE' }">selected="selected"</c:if>>최종접속일</option>										<option value="INACTIVITY_DATE" <c:if test="${resultParam.srch_reg_type eq 'INACTIVITY_DATE' }">selected="selected"</c:if>>휴면전환일</option>									</select>															    <input type="text" name="srch_reg_s" class="ipt2 datepicker" value="${resultParam.srch_reg_s }" maxlength="10"/>								 	~ 								 	<input type="text" name="srch_reg_e" class="ipt2 datepicker" value="${resultParam.srch_reg_e }" maxlength="10"/>								</td>							</tr>							<tr>								<th colspan="4">									<div class="tb_btn_area center">										<button class="btn_pack btn_mo d_gray" onclick="doSearch();" id="search">Search</button>									</div>								</th>							</tr>						</tbody>					</table>				</div>			</form>			<div class="tb_type1">				<h3>검색결과</h3>				<div class="result_summary">Total : <strong>${totalCount }</strong></div>				<table>				 	<colgroup>				 		<col width="10%">				 		<col width="10%">				 		<col>				 		<col>				 		<col>				 		<col>				 		<col width="10%">				 	</colgroup>				 	<thead>				 		<tr>							<th>회원번호</th>							<th>회원명</th>							<th>ID</th>							<th>최종접속일</th>							<th>휴면전환</th>							<th>메일발송</th>							<th>상	태</th>						</tr>				 	</thead>					<tbody>						<c:forEach items="${mList }" var="vo">						<tr>							<td>${vo.MEM_NUM }</td>							<td>${vo.MEM_NM }</td>							<td>								<c:choose>									<c:when test="${MEMINFOYN eq 'Y'}">										${vo.MEM_ID }																			</c:when>									<c:otherwise>										${vo.MASK_MEM_ID }									</c:otherwise>								</c:choose>															</td>							<td>${vo.LAST_LOGIN_DATE}</td>							<td>${vo.INACTIVITY_DATE}</td>							<td>${vo.SEND_MAIL_DATE}</td>							<td>								<c:if test="${vo.INACTIVITY_AT eq 'N' }">대기</c:if>								<c:if test="${vo.INACTIVITY_AT eq 'Y' }">휴면</c:if>							</td>						</tr>						</c:forEach>				 		<c:if test="${empty mList }">				 			<tr>				 				<td colspan="7"> 조회된 내용이 없습니다. </td>				 			<tr>				 		</c:if>
					</tbody>				</table>			</div>			<div class="paginate">				<c:if test="${pu.blockStartNum > 1 }">					<a href="javascript:;" onclick="doPaging('${pu.blockStartNum - 1 }');" class="btn prev disable">PREV</a>				</c:if>				<c:forEach var="i" begin="${pu.blockStartNum}" end="${pu.blockEndNum }">					<a href="javascript:;" onclick="doPaging('${i}');" class="num <c:if test="${i == pu.pageNum }">on</c:if>">${i}</a>				</c:forEach>				<c:if test="${pu.blockEndNum < pu.pageTotalCount }">					<a href="javascript:;" onclick="doPaging('${pu.blockEndNum + 1 }');" class="btn next">NEXT</a>				</c:if>			</div>		</div>	</div></section>
<script type="text/javascript">	//검색	function doSearch(){		if(!dateValidation("srch_reg_s")) return false;		if(!dateValidation("srch_reg_e")) return false;		if(!dateToDate("srch_reg_s", "srch_reg_e")) return false;		document.serach_form.submit();	};			//페이징	function doPaging(num){		$("input[name='page']").val(num);		doSearch();	};</script>