<%@ page contentType="text/html; charset=utf-8" pageEncoding="utf-8"%><%@ include file="../../common/taglibs.jsp" %>
<section id="contents">	<div class="contents_bx_type1">		<div class="contents_bx_inner">			<div id="path" class="fixed">				<div class="pathInner">					<h2>SMS 발송관리</h2>				</div>			</div>			<div class="tb_type1">				<h3>SMS 목록</h3>				<table>				 	<colgroup> 						<col width=15%/> 						<col width=75%/> 						<col width=10%/> 					</colgroup>				 	<thead>				 		<tr>							<th>구	분</th>							<th>내	용</th>							<th>관	리</th>						</tr>				 	</thead>					<tbody><c:forEach items="${results}" var="result" varStatus="idx">						<tr>							<td>								<strong>								 	<c:if test="${result.SMS_TYPE eq 'JOIN'}">회원가입</c:if>									<c:if test="${result.SMS_TYPE eq 'RESERVE'}">예약완료</c:if>									<c:if test="${result.SMS_TYPE eq 'CANCEL'}">예약취소</c:if>									<c:if test="${result.SMS_TYPE eq 'BEFORE'}">방문1일전 알림</c:if>									<c:if test="${result.SMS_TYPE eq 'CERTI'}">문자인증</c:if>																	</strong>							</td>							<td class='tit'>${result.SMS_CONTENT}</td>							<td><a href="sms_write.af?num=${result.SMS_UID}" class="link">수정</a></td>						</tr></c:forEach>					</tbody>				</table>			</div>		</div>	</div></section><script type="text/javascript"></script>