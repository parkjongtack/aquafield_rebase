<%@ page contentType="text/html; charset=utf-8" pageEncoding="utf-8"%><%@ taglib uri="http://tiles.apache.org/tags-tiles" prefix="tile" %><%@ include file="../../common/taglibs.jsp" %><tile:importAttribute name="MEMINFO"/><section id="password">    <div class='inner'>    	<div class="pw_box">	        <div class="inner">	            <h1>비밀번호 확인</h1>	            <p>※ 안전한 개인정보 관리를 위해 비밀번호를 다시 한번 입력해주세요.</p>	            <form name="submitForm" id="submitForm" method="post" onsubmit="return false;">	                <div class="form_type1">	                    <ul>	                        <li>	                            <div class="placeh">	                                <input type="password" class="ipt bd_hide w100p" name="member_pw" id="member_pw" value="" onblur="chkAccount.pw01('#member_pw')">	                                <label for="member_pw">Password</label>	                            </div>	                        </li>	                    </ul>	                    <!-- <div class="status">> 패스워드를 정확히 입력해주세요.</div> -->	                </div>	            </form>	            <div class="btns center">	                <button class="btn_pack btn_mo brown" onclick="submit();">확인</button>	            </div>	        </div>	    </div>    </div>    <script type="text/javascript">        placeHFn.align();   	       	    $( "#member_pw" ).keypress(function(e) {            if (e.which == 13) {                submit();            }        });   	    function submit(){    	   	    	if(valCheck()){	   	    	var formData = $("#submitForm").serialize();		    	$.ajax({			   		async: false			   		,type: "post"			   		,url : '/mypage/proccess.af'			   		,data : formData			   		,dataType : "html"			   		,success: function(obj){						$("#password").append(obj);			   		}			   		,error: function(xhr, option, error){			   			alert("에러가 발생했습니다. 잠시 후에 다시하세요.");			   		}		   		});   	    	}	    };	    	   function valCheck(){		   if($.trim($("#member_pw").val()) == "" || $("#member_pw").val() == null){			   alert('사용자 패스워드를 입력해 주세요.');			   $("#member_pw").focus();			   return false;		   }		   		   return true;	   };     </script></section>