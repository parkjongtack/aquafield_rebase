        <script type="text/javascript">
            $('.customized-select').customSelect();

            $('.accordion .panel').each(function(e) {
                var _this = $(this),
                    content = $(this).find('.panel-content'),
                    contentH = 0,
                    moving = true;

                $(this).find('.panel-haed').click(function(argument) {
                    if(moving){
                        moving = false;
                        if(!_this.hasClass('active')){
                            $('.accordion .panel.active').removeClass('active').find('.panel-content').animate({height: 0},500, function() {
                                $(this).css({height: '', display: ''});
                            });

                            _this.addClass('active');
                            contentH = content.height();
                            content.css({height: 0, display: 'block'}).animate({height: contentH},500, function() {
                                $(this).css({height: ''});
                                iscrollFn.refresh();
                                moving = true;
                            });
                        }else{
                            _this.removeClass('active');
                            content.animate({height: 0},500, function() {
                                 $(this).css({height: '', display: ''});
                                iscrollFn.refresh();
                                moving = true;
                            });
                        }
                    } 
                });
            });

            $('.catelist ul > li a').click(function(e) {
                if(!$(this).hasClass('active')){
                    var t = $(this).attr('id');
                    $('.catelist ul > li a').removeClass('active')
                    $(this).addClass('active');

                    $('.panel').each(function(i, v) {
                        if($(this).hasClass(t)){
                            $(this).css('display', 'block');
                        }else if(t == 's0') {
                            $(this).css('display', 'block');
                        }else{
                            $(this).css('display', 'none');
                        }
                    });
                    iscrollFn.refresh();
                }
            });

        </script>
