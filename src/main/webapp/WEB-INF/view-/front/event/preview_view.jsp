<%@ page contentType="text/html; charset=utf-8" pageEncoding="utf-8"%><%@ include file="../../common/taglibs.jsp" %><section id="notice_view">	<header>        <div class="titleArea">        	<div class="tag"><img class="ico" src="/common/front/images/ico/ico_news_notice.png" alt="공지">        	 <span id="kindTxt"></span>        	 </div>        	<h1 class="title"></h1>        	<ul class="info">        		<li id="instDate"><span>작성일: </span></li>        		<li id="startDate"><span>기간: </span></li>        	</ul>        	<ul class="sns"><!--         		<li><a href="javascript:void(0);" class="link"></a></li> -->        		<li><a href="javascript:void(0);" class="facebook" onclick="sendSns('facebook','')"></a></li>        		<li><a href="javascript:void(0);" class="twitter" onclick="sendSns('twitter','')"></a></li>        		<li><a href="javascript:void(0);" class="kakao" onclick="sendSns('kakao','')"></a></li>        		<li><a href="javascript:void(0);" class="url" onclick="urlCopy()"></a></li>        	</ul>        </div>    </header>	<div class="iscContent">        <div class="inner">        	<article>        		<div class="newsContent">        		</div>        	</article>        </div>    </div>    <footer>    </footer></section><script type="text/javascript">function previewView(){	var previewKind = "";	var previewTitle = "";	var previewInstDate = "";	var previewStartDate = "";	var previewContent = "";	if($(':radio[name="KIND"]:checked').val() == 1) previewKind = "공지사항";	else  previewKind = "이벤트";	previewTitle = document.form.TITLE.value;	var myYear = new Date().getFullYear();	var myMonth = new Date().getMonth() + 1;	var myDay = new Date().getDate();	if(myMonth < 10) myMonth = "0" + myMonth;	if(myDay < 10) myDay = "0" + myDay;	previewInstDate = "<span>작성일: </span>" + myYear + "." + myMonth + "." + myDay;	previewStartDate = "<span>기간: </span>" + document.form.START_DATE.value + (document.form.END_DATE.value == "" ? "":" ~ " + document.form.END_DATE.value);	if($(':radio[name="WRITE_TYPE"]:checked').val() == 1){		previewContent = $("#imgArea_2").html();	}	else{		oEditors.getById["CONTENTS"].exec("UPDATE_CONTENTS_FIELD", []);		previewContent = document.form.CONTENTS.value;	}	$("#kindTxt").html(previewKind);	$(".title").html(previewTitle);	$("#instDate").html(previewInstDate);	$("#startDate").html(previewStartDate);	$(".newsContent").html(previewContent);}previewView();</script>