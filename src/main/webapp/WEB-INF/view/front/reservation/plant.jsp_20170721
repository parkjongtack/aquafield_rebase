<%@ page contentType="text/html; charset=utf-8" pageEncoding="utf-8"%><%@ include file="../../common/taglibs.jsp" %><button class="btn_close">닫기</button><div id="reservation_plant" class="in inc_abs_foot_btn">    <c:choose>    	<c:when test="${!empty prodInfos}">	    <header class="hd_type2">	        <h2>사용시설과 예약인원을 설정 해주세요</h2>	    </header>		   	<form method="post" onsubmit="return false;" class="myform">		    <section class="form_type_res">		        <div class="limit_count_box total" data-limit="10">		            <img class="ico" src="/common/front/images/ico/ico_res_limit.png"/>최대 예약가능 인원 <span>10</span>명		        </div>		        <c:if test="${spaList ne null && spaList ne ''}">		        <div class="item">		            <h3>${spaList.ITEM_NM}</h3>		            <ul class="spa" data-limit="${spaList.QUANTITY}">		                <li data-code="spa1" data-title="${spaList.ITEM_NM}" data-age="(대인)" data-uid="${spaList.SET_UID}" >		                    <input type="hidden" name="price" class="price" value="${spaList.ADULTS_PRICE}">		                    <span class="tit">대 인 : </span>		                    <p><label>남</label>		                    <input type="number" name="spaman1" class="iptNum man"></p>		                    <p><label>여</label>		                    <input type="number" name="spawoman1" class="iptNum woman"></p>		                    <div class="itmeTotal"></div>		                </li>		                <li data-code="spa2" data-title="${spaList.ITEM_NM}" data-age="(소인)" data-uid="${spaList.SET_UID}">		                    <input type="hidden" name="price" class="price" value="${spaList.CHILD_PRICE}">		                    <span class="tit">소 인 : </span>		                    <p><label>남</label>		                    <input type="number" name="spaman2" class="iptNum man"></p>		                    <p><label>여</label>		                    <input type="number" name="spawoman2" class="iptNum woman"></p>		                    <div class="itmeTotal"></div>		                </li>		            </ul>		        </div>		        </c:if>		        		         <c:if test="${wpList ne null && wpList ne ''}">		        <div class="item">		            <h3>${wpList.ITEM_NM}</h3>		            <ul class="water" data-limit="${wpList.QUANTITY}">		                <li data-code="water1" data-title="${wpList.ITEM_NM}" data-age="(대인)" data-uid="${wpList.SET_UID}" >		                    <input type="hidden" name="price" class="price" value="${wpList.ADULTS_PRICE}">		                    <span class="tit">대 인 : </span>		                    <p><label>남</label>		                    <input type="number" name="waterman1" class="iptNum man"></p>		                    <p><label>여</label>		                    <input type="number" name="waterwoman1" class="iptNum woman"></p>		                    <div class="itmeTotal"></div>		                </li>		                <li data-code="water2" data-title="${wpList.ITEM_NM}" data-age="(소인)" data-uid="${wpList.SET_UID}">		                    <input type="hidden" name="price" class="price" value="${wpList.CHILD_PRICE}">		                    <span class="tit">소 인 : </span>		                    <p><label>남</label>		                    <input type="number" name="waterman2" class="iptNum man"></p>		                    <p><label>여</label>		                    <input type="number" name="waterwoman2" class="iptNum woman"></p>		                    <div class="itmeTotal"></div>		                </li>		            </ul>		        </div>		        </c:if>		        		        <c:if test="${complexList ne null && complexList ne ''}">		        <div class="item">		            <h3>${complexList.ITEM_NM}</h3>		            <ul class="complex" data-limit="${complexList.QUANTITY}">		                <li data-code="complex1" data-title="${complexList.ITEM_NM}" data-age="(대인)" data-uid="${complexList.SET_UID}" >		                    <input type="hidden" name="price" class="price" value="${complexList.ADULTS_PRICE}">		                    <span class="tit">대 인 : </span>		                    <p><label>남</label>		                    <input type="number" name="complexman1" class="iptNum man"></p>		                    <p><label>여</label>		                    <input type="number" name="complexwoman1" class="iptNum woman"></p>		                    <div class="itmeTotal"></div>		                </li>		                <li data-code="complex2" data-title="${complexList.ITEM_NM}" data-age="(소인)" data-uid="${complexList.SET_UID}">		                    <input type="hidden" name="price" class="price" value="${complexList.CHILD_PRICE}">		                    <span class="tit">소 인 : </span>		                    <p><label>남</label>		                    <input type="number" name="complexman2" class="iptNum man"></p>		                    <p><label>여</label>		                    <input type="number" name="complexwoman2" class="iptNum woman"></p>		                    <div class="itmeTotal"></div>		                </li>		            </ul>		        </div>		        </c:if>		    </section>		    </form>		    <footer class="btns center">		    	<section class="notice" style="display:none;">		    		<ul>		    			<li>대인 : 중학생이상  / 소인 : 36개월 ~ 초등학생</li>		    			<li>36개월 미만 유아는 무료입장이 가능합니다.<br/> (유아/소인 입장 시 증빙서류를 제시 바랍니다.)</li>		    		</ul>		    	</section>		        <section class="form_type_res_total">		            <span class="tit">합계</span>		            <span class="price">0원</span>		        </section>		        <button onclick="reservationPop.setResData('item','.item li');" class="btn_pack wide btn_mo blue"><span>확인</span> </button>		    </footer>		    <script type="text/javascript">		        reservationPop.getResData('item','.item li');		        $(".iptNum").customNumber();		        $(".iptNum").change(function(event) {		            calculation(this);		        });		        calculation();		    </script>    	</c:when>    	<c:otherwise>	    <header class="hd_type2">	        <h2>준비된 상품을 없습니다.</h2>	    </header>    	</c:otherwise>    </c:choose></div>