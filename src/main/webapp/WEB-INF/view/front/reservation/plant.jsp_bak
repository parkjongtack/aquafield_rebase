<%@ page contentType="text/html; charset=utf-8" pageEncoding="utf-8"%><%@ include file="../../common/taglibs.jsp" %><button class="btn_close">닫기</button><div id="reservation_plant" class="in inc_abs_foot_btn">    <c:choose>    	<c:when test="${!empty prodInfos}">	    <header class="hd_type2">	        <h2>사용시설과 예약인원을 설정해 주세요.</h2>	    </header>    			   	<form method="post" onsubmit="return false;" class="myform">		    <section class="form_type_res">		        <div class="limit_count_box total" data-limit="10">		            <img class="ico" src="/common/front/images/ico/ico_res_limit.png"/>예약가능 인원 <span>10</span>명		        </div>		       <c:forEach items="${prodInfos}" var="prodInfo" varStatus="status">			        <c:if test="${prodInfo.ITEM_CODE eq 'PRD001'}"> <c:set value="spa" var="type"/></c:if>			        <c:if test="${prodInfo.ITEM_CODE eq 'PRD002'}"> <c:set value="water" var="type"/></c:if>			        <c:if test="${prodInfo.ITEM_CODE eq 'PRD003'}"> <c:set value="complex" var="type"/></c:if> 			        <div class="item">			            <h3>${status.count}. ${prodInfo.ITEM_NM}</h3>			            <ul class="${type}" data-limit="${prodInfo.QUANTITY}">			                <li data-code="${type}1" data-title="${prodInfo.ITEM_NM}" data-age="(대인)" data-uid="${prodInfo.SET_UID}" >			                    <input type="hidden" name="price" class="price" value="${prodInfo.ADULTS_PRICE}">			                    <span class="tit">대 인 : </span>			                    <p><label>남</label>			                    <input type="number" name="${type}man1" class="iptNum man"></p>			                    <p><label>여</label>			                    <input type="number" name="${type}woman1" class="iptNum woman"></p>			                    <div class="itmeTotal"></div>			                </li>			                <li data-code="${type}2" data-title="${prodInfo.ITEM_NM}" data-age="(소인)" data-uid="${prodInfo.SET_UID}">			                    <input type="hidden" name="price" class="price" value="${prodInfo.CHILD_PRICE}">			                    <span class="tit">소 인 : </span>			                    <p><label>남</label>			                    <input type="number" name="${type}man2" class="iptNum man"></p>			                    <p><label>여</label>			                    <input type="number" name="${type}woman2" class="iptNum woman"></p>			                    <div class="itmeTotal"></div>			                </li>			            </ul>		        	</div>		        </c:forEach>		    </section>		    </form>		    <footer class="btns center">		        <section class="form_type_res_total">		            <span class="tit">합계</span>		            <span class="price">0원</span>		        </section>		        <button onclick="reservationPop.setResData('item','.item li');" class="btn_pack wide btn_mo blue"><span>확인</span> </button>		    </footer>		    <script type="text/javascript">		        reservationPop.getResData('item','.item li');		        $(".iptNum").customNumber();		        $(".iptNum").change(function(event) {		            calculation(this);		        });		        calculation();		    </script>    	    	</c:when>    	<c:otherwise>	    <header class="hd_type2">	        <h2>준비된 상품이 없습니다.</h2>	    </header>      	    	</c:otherwise>    </c:choose></div>