<%@ page contentType="text/html; charset=utf-8" pageEncoding="utf-8"%><%@ include file="../../common/taglibs.jsp" %><button class="btn_close">닫기</button><div id="reservation_rental" class="in inc_abs_foot_btn">    <c:choose>    	<c:when test="${!empty prodInfos}">		    <header class="hd_type2">		        <h2>대여상품 추가 예약</h2>		    </header>		   	<form method="post" onsubmit="return false;">		    <section class="form_type_res">		        <div class="notice">※ 대여상품은 필수 구매사항이 아닙니다.</div>	            <c:forEach items="${prodInfos}" var="prodInfo" varStatus="status">		        <div class="limit_count_box" data-limit="${prodInfo.LIMIT}">		            <img class="ico" src="/common/front/images/ico/ico_res_limit.png"/>최대 예약가능 인원 <span>${prodInfo.LIMIT}</span>명		        </div>	            		        <ul class="item">		            			            <li data-code="rantal${status.count}" data-title="${prodInfo.ITEM_NM}" data-uid="${prodInfo.SET_UID}">			                <input type="hidden" name="" class="price" value="${prodInfo.RENTAL_PRICE}">			                <span class="tit">${prodInfo.ITEM_NM}</span>			                <input type="number" name="rantal${status.count}" class="iptNum resData">			                <div class="itmeTotal"></div>			            </li> 		        </ul>			                       		        </c:forEach>		    </section>		    </form>		    <footer class="btns right">		        <section class="form_type_res_total">		            <span class="tit">합계</span>		            <span class="price">0원</span>		            <input type="hidden" name="totalprice" class="totalprice resData">		        </section>		        <button onclick="reservationPop.setResData('item','.item li');" class="btn_pack wide btn_mo blue"><span>확인</span> </button>		    </footer>		    <script type="text/javascript">		        reservationPop.getResData('item','.item li');		        $(".iptNum").customNumber();		        $(".iptNum").change(function(event) {		            calculation();		        });		        calculation();		    </script>    	</c:when>    	<c:otherwise>	    <header class="hd_type2">	        <h2>대여 상품이 없습니다.</h2>	    </header>      	    	</c:otherwise>    </c:choose>			    </div>