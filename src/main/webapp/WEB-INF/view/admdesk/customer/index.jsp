<%@ page contentType="text/html; charset=utf-8" pageEncoding="utf-8"%><%@ include file="../../common/taglibs.jsp" %><script type="text/javascript"><!--	$( document ).ready(function() {		// msg		if("${msg}" != ""){			alert("${msg}");		}	});	//검색	function doSearch(){		if(!dateValidation("srch_reg_s")) return false;		if(!dateValidation("srch_reg_e")) return false;		if(!dateToDate("srch_reg_s", "srch_reg_e")) return false;		document.serach_form.submit();	};			//페이징	function doPaging(num){		$("input[name='page']").val(num);		doSearch();	};		//상세	function goDetail(seq){		var frm = document.serach_form;		$("input[name='ask_uid']").val(seq);		frm.action = "/secu_admaf/admdesk/customer/contact_view.af"; 		frm.submit();	}	//--> </script><section id="contents">	<div class="contents_bx_type1">		<div class="contents_bx_inner">			<div id="path" class="fixed">				<div class="pathInner">					<h2>고객의 소리</h2>				</div>			</div>			<form name="serach_form" action="/secu_admaf/admdesk/customer/index.af" method="post" onsubmit="return false;">				<input type="hidden" name="page" value="${resultParam.page }">				<input type="hidden" name="ask_uid" value="">				<div class="tb_type2">					<h3>조건검색</h3>					<table>						<tbody>							<tr>								<th>회원정보</th>								<td><input type="text" name="srch_text" class="ipt2 w100p" value="${resultParam.srch_text }"  onkeydown="javascript:if(event.keyCode==13){doSearch();}"/></td>								<th>지점</th>								<td>									<select name="srch_point" id="srch_point" class="customized-select">										<option value="">전체</option>										<c:forEach items="${codePoint_code }" var="code" >											<option value="${code.CODE_ID }" <c:if test="${resultParam.srch_point == code.CODE_ID }">selected="selected"</c:if> >${code.CODE_NM }</option>										</c:forEach>									</select>								</td>							</tr>							<tr>								<th>회원유형</th>								<td>									<select name="srch_mem" id="srch_mem" class="customized-select" style="width: 100px">										<option value="">전체</option>										<c:forEach items="${codeMEM_TYPE }" var="code" >											<option value="${code.CODE_ID }" <c:if test="${resultParam.srch_mem == code.CODE_ID }">selected="selected"</c:if> >${code.CODE_NM }</option>										</c:forEach>									</select>								</td>																								<th>문의유형</th>								<td>									<select name="srch_type" id="srch_type" class="customized-select" style="width: 100px">										<option value="">전체</option>										<c:forEach items="${codeASK_TYPE }" var="code" >											<option value="${code.CODE_ID }" <c:if test="${resultParam.srch_type == code.CODE_ID }">selected="selected"</c:if> >${code.CODE_NM }</option>										</c:forEach>									</select>								</td>							</tr>							<tr>								<th>등록일</th>								<td>									<input type="text" name="srch_reg_s" class="ipt2 datepicker" value="${resultParam.srch_reg_s }" maxlength="10"/>									 ~ 									 <input type="text" name="srch_reg_e" class="ipt2 datepicker" value="${resultParam.srch_reg_e }" maxlength="10"/>								</td>								<th>처리상태</th>								<td>									<select name="srch_stat" id="srch_stat" class="customized-select">										<option value="">전체</option>										<c:forEach items="${codeASK_STAT }" var="code" >											<option value="${code.CODE_ID }" <c:if test="${resultParam.srch_stat == code.CODE_ID }">selected="selected"</c:if> >${code.CODE_NM }</option>										</c:forEach>									</select>								</td>							</tr>							<tr>								<th colspan="4">									<div class="tb_btn_area center">										<button class="btn_pack btn_mo d_gray" type="button" onclick="doSearch();">Search</button>									</div>								</th>							</tr>						</tbody>					</table>				</div>			</form>			<div class="tb_type1">				<h3>검색결과</h3>				<div class="result_summary">Total : <strong>${totalCount }</strong></div>				<table>				 	<colgroup> 						<col width="5%/"> 						<col width="6%/"> 						<col width="8%/"> 						<col width="8%/"> 						<col width="15%/"> 						<col width="30%/"> 						<col width="15%/"> 						<col width="8%/"> 					</colgroup>				 	<thead>				 		<tr>							<th>No.</th>							<th>지점</th>							<th>문의유형</th>							<th>회원명</th>							<th>ID</th>							<th>제목</th>							<th>등록일</th>							<th>처리상태</th>						</tr>				 	</thead>					<tbody>						<c:forEach items="${cList }" var="vo" >							<tr>								<td>${vo.ASK_UID }</td>								<td>${vo.POINT }</td>								<td>${vo.ASK_TYPE_NM }</td>								<td>									<c:choose>										<c:when test="${MEMINFOYN eq 'Y'}">											${vo.WRITER }																				</c:when>										<c:otherwise>											${vo.MASK_WRITER }										</c:otherwise>									</c:choose>																	</td>								<td>									<c:choose>										<c:when test="${MEMINFOYN eq 'Y'}">											${vo.INS_ID }																				</c:when>										<c:otherwise>											${vo.MASK_INS_ID }										</c:otherwise>									</c:choose>									</td>								<td class="tit"><a href="javascript:;" onclick="goDetail('${vo.ASK_UID}');" class="link">${vo.ASK_TITLE }</a></td>								<td>${vo.INS_DATE }</td>								<c:choose>									<c:when test="${vo.ASK_STAT == 2 }">										<c:set var="color" value="cancel" />										<c:set var="statusNm" value="답변요청" />									</c:when>									<c:otherwise>										<c:set var="color" value="res" />										<c:set var="statusNm" value="답변완료" />									</c:otherwise>								</c:choose>																	<td class="stat ${color }">																${statusNm}								</td>							</tr>						</c:forEach>						<c:if test="${empty cList }">				 			<tr>				 				<td colspan="8"> 조회된 내용이 없습니다. </td>				 			<tr>				 		</c:if>					</tbody>				</table>			</div>			<div class="paginate">				<c:if test="${pu.blockStartNum > 1 }">					<a href="javascript:;" onclick="doPaging('${pu.blockStartNum - 1 }');" class="btn prev disable">PREV</a>				</c:if>				<c:forEach var="i" begin="${pu.blockStartNum}" end="${pu.blockEndNum }">					<a href="javascript:;" onclick="doPaging('${i}');" class="num <c:if test="${i == pu.pageNum }">on</c:if>">${i}</a>				</c:forEach>				<c:if test="${pu.blockEndNum < pu.pageTotalCount }">					<a href="javascript:;" onclick="doPaging('${pu.blockEndNum + 1 }');" class="btn next">NEXT</a>				</c:if>			</div>		</div>	</div></section>