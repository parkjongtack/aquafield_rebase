<%@ page contentType="text/html; charset=utf-8" pageEncoding="utf-8"%><%@ include file="../../common/taglibs.jsp" %><section id="contents">	<div class="contents_bx_type1">		<div class="contents_bx_inner">			<div id="path" class="fixed">				<div class="pathInner">					<h2>접속통계</h2>				</div>			</div>			<div class="condi_search inc_date">		 		<ul class="clearfix">					<li class="on"><a href="/secu_admaf/admin/statistics/index.af">Hour</a></li>					<li><a href="/secu_admaf/admin/statistics/day.af">Day</a></li>					<li><a href="/secu_admaf/admin/statistics/month.af">Month</a></li>				</ul>				<form name="searchForm" id="searchForm" method="post" action="/secu_admaf/admin/statistics/index.af" onsubmit="return false;">				<div class="condi_date multi_search">					<ul>						<li class="on">							<input type="text" name="vd" id="vd" class="ipt2 datepicker" value="${resultParam.vd}">						</li>					</ul>				<button type="button" id="btnSearch" class="btn_pack blue" onclick="searchRun();">Search</button>				<div class="clear"></div>		 		</div>		 		</form>		 	</div>	 		<div class="area_graph">				<div id="chart_div" style="height:800px;">그래프 영역</div>		 	</div>		</div>	</div></section><script type="text/javascript">google.charts.setOnLoadCallback(drawChart);function drawChart() {	<c:if test="${not empty results}">	var data = google.visualization.arrayToDataTable([	                                                  		['시각', '접속통계'],		['0:00', ${results.HOUR_00}],		['1:00', ${results.HOUR_01}],		['2:00', ${results.HOUR_02}],		['3:00', ${results.HOUR_03}],		['4:00', ${results.HOUR_04}],		['5:00', ${results.HOUR_05}],		['6:00', ${results.HOUR_06}],		['7:00', ${results.HOUR_07}],		['8:00', ${results.HOUR_08}],		['9:00', ${results.HOUR_09}],		['10:00', ${results.HOUR_10}],		['11:00', ${results.HOUR_11}],		['12:00', ${results.HOUR_12}],		['13:00', ${results.HOUR_13}],		['14:00', ${results.HOUR_14}],		['15:00', ${results.HOUR_15}],		['16:00', ${results.HOUR_16}],		['17:00', ${results.HOUR_17}],		['18:00', ${results.HOUR_18}],		['19:00', ${results.HOUR_19}],		['20:00', ${results.HOUR_20}],		['21:00', ${results.HOUR_21}],		['22:00', ${results.HOUR_22}],		['23:00', ${results.HOUR_23}]		]);	</c:if>		var options = {		//title: '',		//vAxis: {title: '시각',  titleTextStyle: {color: 'red'}}	};	var chart = new google.visualization.BarChart(document.getElementById("chart_div"));    chart.draw(data, options);	}</script><script>function searchRun(){	if($.trim(document.searchForm.vd.value) == ""){		alert("검색일을 선택하세요.");		document.searchForm.vd.focus();		return;	}	document.getElementById("searchForm").submit();};  </script></div><section id="popup_write"></section>