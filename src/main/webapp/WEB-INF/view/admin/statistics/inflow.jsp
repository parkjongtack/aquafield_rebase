<%@ page contentType="text/html; charset=utf-8" pageEncoding="utf-8"%><%@ include file="../../common/taglibs.jsp" %><section id="contents"><div class="contents_bx_type1">	<div class="contents_bx_inner">		<div id="path" class="fixed">			<div class="pathInner">				<h2>유입경로</h2>			</div>		</div>		<form name="serach_form" action="/secu_admaf/admin/statistics/inflow.af" method="post" onsubmit="search();return false;">			<input type="hidden" name="page" value="${resultParam.page }">			<input type="hidden" name="daytype" value="date">			<div class="tb_type2">				<h3>조건검색</h3>				<table>					<colgroup>						<col width="10%">						<col width="40%">						<col width="10%">						<col width="40%">					</colgroup>					<tbody>						<tr>							<th>기	간 </th>							<td>								<div class="lst_check radio">									<span <c:if test="${resultParam.daytype eq 'today'}">class="on"</c:if>>										<label>											금	일<input type="radio" name="date" id="today" value="today" onclick="checkradio(this);" <c:if test="${resultParam.daytype eq 'today'}">checked</c:if>>										</label>									</span>									<span <c:if test="${resultParam.daytype eq 'week'}">class="on"</c:if>>										<label>											1주일<input type="radio" name="date" id="week" value="week" onclick="checkradio(this);" <c:if test="${resultParam.daytype eq 'week'}">checked</c:if>>										</label>									</span>									<span <c:if test="${resultParam.daytype eq 'month'}">class="on"</c:if>>										<label>											한	달<input type="radio" name="date" id="month" value="month" onclick="checkradio(this);" <c:if test="${resultParam.daytype eq 'month'}">checked</c:if>>										</label>									</span>								</div>							</td>							<th>직접입력</th>							<td>								<input type="text" name="srch_date_s" value="${resultParam.srch_date_s}" class="ipt2 datepicker" onchange="selfInput();"/> ~ <input type="text" name="srch_date_e" value="${resultParam.srch_date_e}" class="ipt2 datepicker" onchange="selfInput();"/>							</td>						</tr>						<tr>							<th colspan="4">								<div class="tb_btn_area center">									<button type="button" class="btn_pack btn_mo d_gray" onclick="doSearch();">Search</button>								</div>							</th>						</tr>					</tbody>				</table>			</div>		</form>			<div class="tb_type1">				<h3>검색결과</h3>				<div class="result_summary">Total : <strong>${totalCount}</strong></div>				<table style="width: 100%; margin-left:0">				 	<colgroup>				 	 	<col width="10%/">				 	 	<col width="70%/">				 	 	<col width="20%/">				 	</colgroup>					<thead>						<tr>							<th>순위</th>							<th>유입경로(URL)</th>							<th>유입 수</th>						</tr>					</thead>					<tbody id="refererView">					  <c:forEach var="vo" items="${refererList}" varStatus="num" >			        	<tr>			        	 <td>${num.count}</td>			        	 <td class="left">${vo.REFERER}</td>			        	 <td>${vo.SUMCNT}</td>			        	</tr>			          </c:forEach>					  <c:if test="${empty refererList }">							<tr>								<td colspan="3">검색된 내역이 없습니다.</td>							</tr>					  </c:if>			          											</tbody>				</table>			</div>			<div class="paginate">				<c:if test="${pu.blockStartNum > 1 }">					<a href="javascript:;" onclick="doPaging('${pu.blockStartNum - 1 }');" class="btn prev disable">PREV</a>				</c:if>				<c:forEach var="i" begin="${pu.blockStartNum}" end="${pu.blockEndNum }">					<a href="javascript:;" onclick="doPaging('${i}');" class="num <c:if test="${i == pu.pageNum }">on</c:if>">${i}</a>				</c:forEach>				<c:if test="${pu.blockEndNum < pu.pageTotalCount }">					<a href="javascript:;" onclick="doPaging('${pu.blockEndNum + 1 }');" class="btn next">NEXT</a>				</c:if>			</div>		</div>	</div>	<script type="text/javascript">	$(function(){		var today = new Date();		var week = new Date();		var month = new Date();		var startDate = today, endDate = today;				//$("input[name='srch_date_s']").datepicker('setDate', startDate);		//$("input[name='srch_date_e']").datepicker('setDate', endDate);				$("input[name='date']").change(function(v) {			var setToday = new Date();			var setWeek = new Date();			var setMonth = new Date();							var setStartDate ="";			var setEndDate = "";			switch($(this).val()) {			    case 'today':			    	setStartDate = setToday;			    	setEndDate = setToday;			        break;			    case 'week':			    	setWeek.setDate(setWeek.getDate()-7);			    	setStartDate = setWeek;			    	setEndDate = setToday;			        break;			    case 'month':			    	setMonth.setDate(setMonth.getDate()-30);			    	setStartDate = setMonth;			    	setEndDate = setToday;			        break;			}												$("input[name='srch_date_s']").datepicker('setDate', setStartDate);			$("input[name='srch_date_e']").datepicker('setDate', setEndDate);		});	});		function doSearch(){		if(!dateValidation("srch_date_s")) return false;		if(!dateValidation("srch_date_e")) return false;		if(!dateToDate("srch_date_s", "srch_date_e")) return false;				$("input[name='daytype']").val($(':radio[name="date"]:checked').val());				document.serach_form.submit();	};		function selfInput(){		$(':radio[name="date"]:checked').attr("checked",false);	}	</script></section>